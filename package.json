{
  "name": "storaji",
  "version": "1.0.0-beta.9",
  "license": "MIT",
  "description": "The Light/Responsive Inventory Management System.",
  "main": "app/main.js",
  "author": {
    "name": "Indomaxim Technology",
    "email": "support@indomaxim.web.id",
    "url": "https://indomaxim.web.id/"
  },
  "build": {
    "appId": "com.indomaxim.storaji",
    "productName": "Storaji",
    "releaseInfo": {
      "releaseNotesFile": "src/electron/releaseNotes.md"
    },
    "publish": [
      {
        "provider": "github",
        "owner": "IndomaximTechID",
        "repo": "storaji"
      }
    ],
    "files": [
      "app/**"
    ],
    "directories": {
      "output": "dist"
    },
    "mac": {
      "category": "public.app-category.business"
    },
    "linux": {
      "category": "Office",
      "target": [
        {
          "target": "AppImage",
          "arch": [
            "x64",
            "ia32"
          ]
        }
      ]
    },
    "win": {
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64",
            "ia32"
          ]
        }
      ]
    },
    "nsis": {
      "allowToChangeInstallationDirectory": true,
      "shortcutName": "Storaji",
      "oneClick": false,
      "perMachine": true,
      "menuCategory": true,
      "artifactName": "StorajiSetup.exe"
    }
  },
  "scripts": {
    "build": "run-s icons -l \"build:app --prod\" \"build:installer {@}\" --",
    "build:app": "run-s \"ng:build {@}\" --",
    "build:installer": "run-s \"builder {@}\" --",
    "builder": "build",
    "electron": "electron app/main.js",
    "icons": "icon-gen -i src/assets/storaji.svg -o src/assets/icons -n ico=storaji,icns=storaji",
    "ng": "ng",
    "ng:build": "ng build",
    "ng:e2e": "ng e2e --no-progress",
    "ng:test": "ng test --watch=false --no-progress",
    "ng:lint": "ng lint",
    "ng:serve": "ng serve",
    "postinstall": "electron-builder install-app-deps",
    "prestart": "run-s build:app",
    "webdriver-update": "webdriver-manager update --versions.chrome=2.34",
    "release": "run-s -l \"build {@} -p always\" --",
    "start": "run-s electron",
    "test": "run-s \"ng:e2e {@}\" ng:lint ng:test --"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~8.2.14",
    "@angular/common": "~8.2.14",
    "@angular/compiler": "~8.2.14",
    "@angular/core": "~8.2.14",
    "@angular/forms": "~8.2.14",
    "@angular/http": "^7.2.16",
    "@angular/platform-browser": "~8.2.14",
    "@angular/platform-browser-dynamic": "~8.2.14",
    "@angular/router": "~8.2.14",
    "@fengyuanchen/datepicker": "~0.6.3",
    "@ngui/auto-complete": "^2.0.0",
    "@ngx-progressbar/core": "^5.3.2",
    "@ngx-progressbar/http": "^5.3.2",
    "@ngx-translate/core": "^11.0.1",
    "@ngx-translate/http-loader": "^4.0.0",
    "angular2-highcharts": "^0.5.5",
    "font-awesome": "~4.7.0",
    "gasparesganga-jquery-loading-overlay": "~1.5.3",
    "highcharts": "~6.0.6",
    "ionicons": "~2.0.1",
    "jquery": "^3.4.1",
    "jquery.transit": "^0.9.12",
    "moment": "^2.24.0",
    "ngx-progressbar": "^2.1.1",
    "notyf": "~2.0.1",
    "numeral": "^2.0.6",
    "offline-js": "^0.7.19",
    "pdfmake": "^0.1.63",
    "rxjs": "~6.4.0",
    "rxjs-compat": "^6.5.4",
    "tslib": "^1.10.0",
    "tus-js-client": "^1.8.0",
    "uikit": "~3.0.0-beta.30",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.803.22",
    "@angular/cli": "~8.3.22",
    "@angular/compiler-cli": "~8.2.14",
    "@angular/language-service": "~8.2.14",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "^5.0.0",
    "electron": "~1.7.10",
    "electron-builder": "~19.54.0",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.15.0",
    "typescript": "~3.5.3"
  }
}
